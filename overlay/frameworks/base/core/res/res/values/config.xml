        <item>8</item>
        <item>8</item>
        <item>13</item>
        <item>18</item>
        <item>18</item>
        <item>18</item>
        <item>30</item>
        <item>30</item>
        <item>40</item>
        <item>48</item>
        <item>50</item>
        <item>55</item>
        <item>60</item>
        <item>65</item>
        <item>70</item>
        <item>75</item>
        <item>80</item>
        <item>88</item>
        <item>93</item>
        <item>96</item>
